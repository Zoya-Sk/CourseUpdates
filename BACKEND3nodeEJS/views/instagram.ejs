<%- include("includes/head.ejs") %>
<body>
    <h1>This page belongs to @<%= data.name %></h1>
    <button>Follow <i class="fa-solid fa-plus"></i> </button>
    <button>Message <i class="fa-solid fa-message"></i> </button>
   
    <p>Followers : <%= data.followers %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Following : <%= data.following %></p>
    <hr>
    <% for(let post of data.posts){ %>
        <img src="<%= post.image %>" alt="some image">
        <p>Likes : <%= post.likes %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Comments : <%= post.comments %></p>
    <% } %>


    <%- include("includes/footer.ejs") %>
    <script src="/app.js"></script>
</body>
</html>